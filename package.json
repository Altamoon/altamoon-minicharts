{
  "name": "altamoon-minicharts",
  "version": "0.3.43",
  "description": "An embeddable online tool that allows to track all the Binance futures markets on one page",
  "main": "npm",
  "style": "style.css",
  "scripts": {
    "lint-pure": "cross-env NODE_ENV=development eslint ./src --ext .ts,.tsx",
    "lint": "npm run lint-pure -- --cache --fix",
    "test": "npm run lint-pure && npx tsc --noemit",
    "build": "SKIP_RUNTIME_TESTS=true NODE_ENV=production webpack --mode=production",
    "compile": "tsc --outDir npm --declaration",
    "start": "SKIP_RUNTIME_TESTS=true NODE_ENV=development webpack serve --mode=development --hot --port=8070",
    "upgrade": "npx npm-check-updates -u -x eslint-config-airbnb-typescript && npm i",
    "patch": "npm t && npm version patch && npm run compile && npm run build && (git commit -am 'chore: Build' || true) && npm publish && git push && git push --tags",
    "minor": "npm t && npm version minor && npm run compile && npm run build && (git commit -am 'chore: Build' || true) && npm publish && git push && git push --tags"
  },
  "author": {
    "name": "Letiliel",
    "email": "dev@xod.me",
    "url": "https://github.com/Letiliel"
  },
  "contributors": [
    {
      "name": "Andrey Gubanov",
      "email": "andrey@gubanov.eu",
      "url": "https://opensource.gubanov.eu/"
    }
  ],
  "license": "AGPL-3.0-or-later",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Altamoon/altamoon-minicharts.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/bootstrap": "^5.1.6",
    "@types/d3": "^7.1.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "expect.js": "^0.3.1",
    "typescript": "^4.5.3",
    "unminified-webpack-plugin": "^3.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "altamoon-binance-api": "^0.0.161",
    "altamoon-types": "^0.0.161",
    "balajs": "^1.0.10",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "d3": "^7.2.1",
    "d3-transition": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-bootstrap-icons": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-intersection-observer": "^8.33.0",
    "react-moment": "^1.1.1",
    "reactstrap": "^9.0.1",
    "stream": "^0.0.2",
    "styled-components": "^5.3.3",
    "use-change": "^1.0.4-beta.2",
    "use-onclickoutside": "^0.4.0"
  }
}
