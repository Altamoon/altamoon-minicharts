{
  "name": "minichart-grid",
  "version": "0.0.20",
  "description": "An embeddable online tool that allows to track all the Binance futures markets on one page",
  "main": "docs/minichartGrid.js",
  "style": "docs/style.css",
  "scripts": {
    "lint-pure": "cross-env NODE_ENV=development eslint ./src --ext .ts,.tsx",
    "lint": "npm run lint-pure -- --cache --fix",
    "test": "npm run lint-pure && npx tsc --noemit",
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development --hot --port=8070",
    "upgrade": "npx npm-check-updates -u && npm i",
    "patch": "npm t && npm version patch && npm run build && (git commit -am 'chore: Build' || true) && npm publish && git push && git push --tags",
    "minor": "npm t && npm version minor && npm run build && (git commit -am 'chore: Build' || true) && npm publish && git push && git push --tags"
  },
  "author": {
    "name": "Letiliel",
    "email": "dev@xod.me",
    "url": "https://github.com/Letiliel"
  },
  "contributors": [
    {
      "name": "Andrey Gubanov",
      "email": "andrey@gubanov.eu",
      "url": "https://opensource.gubanov.eu/"
    }
  ],
  "license": "AGPL-3.0-or-later",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Letiliel/minichart-grid.git"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/d3": "^7.0.0",
    "@types/lodash": "^4.14.173",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "balajs": "^1.0.10",
    "biduul-binance-api": "^0.0.63",
    "buffer": "^6.0.3",
    "d3": "^7.0.3",
    "d3-transition": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "reactstrap": "^8.10.0",
    "stream": "^0.0.2",
    "styled-components": "^5.3.1",
    "use-change": "^0.2.6"
  }
}
